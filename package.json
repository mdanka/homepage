{
  "name": "mdanka-homepage",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Miklos Danka",
  "scripts": {
    "build": "npm-run-all createBuildDir -s bundle:prod",
    "buildDev": "npm-run-all createBuildDir -s bundle:dev ",
    "clean": "rimraf dist",
    "start": "npm-run-all createBuildDir -p watch:server",
    "watch": "npm-run-all createBuildDir -p watch:bundle",
    "bundle:dev": "webpack -d",
    "bundle:prod": "webpack --config webpack.config.js",
    "createBuildDir": "mkdirp dist",
    "watch:bundle": "webpack -d -w",
    "watch:server": "webpack-dev-server -d --config webpack.config.server.js"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.4.0",
    "lodash": "^4.2.0",
    "normalize.css": "^5.0.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-logger": "^1.1.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "@types/history": "^4.5.0",
    "@types/lodash": "^4.14.50",
    "@types/moment": "^2.13.0",
    "@types/react": "^15.0.1",
    "@types/react-addons-css-transition-group": "^15.0.0",
    "@types/react-addons-shallow-compare": "^0.14.18",
    "@types/react-addons-transition-group": "^0.14.18",
    "@types/react-dom": "^0.14.20",
    "@types/react-router": "^2.0.45",
    "@types/whatwg-fetch": "^0.0.33",
    "autoprefixer": "^6.6.0",
