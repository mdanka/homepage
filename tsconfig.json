{
    "compilerOptions": {
        "declaration": true,
        "importHelpers": true,
        "jsx": "react",
        "lib": [
          "dom",
          "es5",
          "es2015"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "build/tmp",
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "ES5"
    },
    "exclude": [
        "build",
        "node_modules"
    ]
}
